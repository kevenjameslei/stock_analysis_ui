# ================= 环境标识 =================
VITE_APP_ENV=development # 可选值: development | staging | production

# ================= 数据接口配置 =================
VITE_API_BASE_URL=https://api.example.com/v1
VITE_WS_URL=wss://realtime.example.com

# ================= 第三方服务密钥 =================
VITE_DATA_PROVIDER_KEY=your_api_key_here
VITE_SENTRY_DSN=https://abc123@sentry.example.com/1
VITE_GA_MEASUREMENT_ID=G-XXXXXXX

# ================= 功能开关 =================
VITE_MOCK_API=true # 是否启用模拟数据
VITE_DEBUG_MODE=true # 调试日志开关
VITE_PERF_MONITOR=true # 性能监控开关

# ================= 图表默认配置 =================
VITE_CHART_THEME=dark # 可选: dark | light
VITE_DEFAULT_TIMEFRAME=1D # 初始时间周期
VITE_MAX_DATA_POINTS=5000 # 最大数据点数量

# ================= 性能优化参数 =================
VITE_WS_RECONNECT_INTERVAL=3000 # WebSocket重连间隔(ms)
VITE_DATA_UPDATE_THROTTLE=1000 # 数据更新节流时间(ms)
VITE_CACHE_TTL=3600000 # 本地缓存有效期(ms)

# ================= 安全配置 =================
VITE_CSP_DIRECTIVES=default-src 'self'; connect-src *
VITE_AUTH_DOMAIN=your-auth-domain.example.com